{
    "name": "patarkf/crawler-cakephp",
    "description": "Crawler, a HTML parser plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Ferreira",
            "email": "paatrickferreira@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Crawler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crawler\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
